@top Program { expression* }
@precedence { JsTag,JsStartTag,JsEndTag,Text }
expression {
  Text |
  JsTag { JsStartTag JsText* JsEndTag }
}
@tokens {
  JsStartTag { "{{" }
  JsEndTag { "}}" }
  JsText { (![}] | "}")+ }
  Text { (![{] | "{")+ }
  @precedence { JsEndTag,JsStartTag,JsText,Text }
}

@detectDelim